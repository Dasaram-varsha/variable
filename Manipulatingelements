        <div id="a">
            <h1>sam</h1>
            <h1>john</h1>
        </div>
        <div id="b">
            <h1>ria</h1>
            <h1>ken</h1>
        </div>
        <script>
            let n=document.createElement("h2")
            n.textContent="hello world"
            let p=document.getElementById("b")
            p.appendChild(n)
            console.log(p.innerHTML)
        </script>

</body>
</html>
