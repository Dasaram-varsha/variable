    <script>
        let expenses;
        let premium;
        let discount=0;
        expenses=prompt("enter the amount")
        premium=prompt("is premium member? (yes or no)")
        if(parseInt(expenses)>100){
            discount=expenses*(0.20);
        }
        else if(parseInt(expenses)>50|| parseInt(expenses)<=100){
            discount=expenses*(0.10);
        }
        else if(premium=="yes"){
            discount=expenses*(0.05);
        }
        let finalAmount =(expenses - discount);
