    <script>
        const t=['abc','pqr','xyz']
        for(let i=0;i<t.length;i++){
            let t1=document.createElement('h1');
            t1.className="a"
            t1.textContent=t[i]
            console.log(t1)
            document.body.appendChild(t1);
        }
    </script>
</body>
